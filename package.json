{
  "name": "match-pro",
  "version": "1.0.0",
  "description": "The most elegant and lightweight pattern matching in pure JavaScript · < 1 KB · 0 deps",
  "type": "module",
  "main": "dist/match.min.js",
  "module": "src/match.js",
  "types": "src/match.d.ts",
  "files": [
    "dist/",
    "src/",
    "match.d.ts"
  ],
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "build": "terser src/match.js -c -m -o dist/match.min.js",
    "bench": "node benchmark/benchmark.js"
  },
  "keywords": [
    "pattern-matching",
    "match",
    "switch",
    "rust-style",
    "zero-deps",
    "tiny"
  ],
  "author": "Juan Cristobal",
  "license": "MIT",
  "repository": "https://github.com/juancristobalgd1/match",
  "devDependencies": {
    "@types/node": "^24.10.1",
    "terser": "^5.31.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.10"
  }
}
